{
  "name": "dynamic-markdown-blog",
  "version": "1.0.0",
  "description": "Dynamic markdown blog with AWS Amplify integration",
  "main": "index.html",
  "scripts": {
    "build": "node build-markdown.js && esbuild app.js --bundle --splitting --outdir=dist --format=esm --minify",
    "dev": "esbuild app.js --bundle --servedir=. --outdir=dist",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "overrides": {
    "@aws-sdk/types": "^3.734.0"
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "marked": "^16.1.2",
    "mermaid": "^11.9.0"
  },
  "keywords": [
    "markdown",
    "blog",
    "static-site",
    "aws-amplify"
  ],
  "author": "Oliver",
  "license": "CC-BY-SA-4.0",
  "devDependencies": {
    "esbuild": "^0.25.8"
  }
}
